{
  "name": "Dropbox Sync",
  "version": "dev",
  "slug": "dropbox_sync",
  "description": "Upload your HomeAssistant backups to Dropbox",
  "url": "https://github.com/peterlindeman/homeassistant-dropbox-sync",
  "startup": "before",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "stdin": true,
  "hassio_api": true,
  "hassio_role": "manager",
  "boot": "auto",
  "map": ["backup", "share"],
  "options": {
    "oauth_access_token": "<YOUR_ACCESS_TOKEN>",
    "output": ""
  },
  "schema": {
    "oauth_access_token": "str",
    "output": "str",
    "keep_last": "int(0,)?",
    "filetypes": "str?"
  },
  "image": "dwelch2101/dropbox-sync-{arch}"
}
